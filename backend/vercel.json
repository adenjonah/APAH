{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/", "dest": "/" },
      { "src": "/museum", "dest": "/museum" },
      { "src": "/museum-images", "dest": "/museum-images" },
      { "src": "/exhibit", "dest": "/exhibit" },
      { "src": "/exhibit-images", "dest": "/exhibit-images" },
      { "src": "/exhibit-videos", "dest": "/exhibit-videos" },
      { "src": "/displayed-locations", "dest": "/displayed-locations" }
    ]
  }